!INTERFILE  :=
!imaging modality := nucmed
name of data file := FBP.v
!GENERAL DATA :=
patient orientation := head_in
patient rotation :=  supine
!GENERAL IMAGE DATA :=
!type of data := Tomographic
imagedata byte order := LITTLEENDIAN

!SPECT STUDY (general) := 
process status := Reconstructed
!number format := float
!number of bytes per pixel := 4
number of dimensions := 3
matrix axis label [1] := x
!matrix size [1] := 129
scaling factor (mm/pixel) [1] := 3.32
matrix axis label [2] := y
!matrix size [2] := 129
scaling factor (mm/pixel) [2] := 3.32
matrix axis label [3] := z
!matrix size [3] := 64
scaling factor (mm/pixel) [3] := 3.32
number of time frames := 1
!END OF INTERFILE :=
