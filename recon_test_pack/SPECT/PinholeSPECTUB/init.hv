!INTERFILE := 
!imaging modality := nucmed
!version of keys := STIR4.0

!GENERAL DATA := 
!name of data file := init.v

!GENERAL IMAGE DATA := 
!type of data := Tomographic
imagedata byte order := LITTLEENDIAN

!SPECT STUDY (general) := 
patient orientation := head_in
patient rotation :=  prone
!process status := reconstructed
!number format := float
!number of bytes per pixel := 4
number of dimensions := 3
matrix axis label [1] := x
matrix axis label [2] := y
matrix axis label [3] := z
!matrix size [1] := 92
!matrix size [2] := 92
!matrix size [3] := 120
scaling factor (mm/pixel) [1] := 0.5
scaling factor (mm/pixel) [2] := 0.5
scaling factor (mm/pixel) [3] := 0.5
number of time frames := 1

!END OF INTERFILE := 
